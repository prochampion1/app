<div class="row">
 <div class="col-sm-8>">
   <% @articles.each do |article|%>
     <div class ="card">

      <div class="card-block">
       <h4 class="card-title"><%= article.title %></h4>
        <p class="card-text"><%= @markdown.render(article.body).html_safe %> </p>
      </div>
      <p>Author: <%=article.user.name%></p>
      </div>
    <%end%>
</div>
</div>